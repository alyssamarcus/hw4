<h1>Login</h1>

<%= form_with :url => "/sessions" do |form| %>
  <div class="mb-3">
   <p>
    <%= form.label "email", class:"form-label" %>
    <%= form.text_field "email", class:"form-control" %>
  </div>

    <%= form.label "password" %>
    <%= form.password_field "password", class: "form-control" %>
  </p>
  </div>
  <div class="row mb-3">
    <div class="col-auto">
    <%= form.submit "Login", class: "btn btn-primary" %>
    </div>
  </div>

<% end %>